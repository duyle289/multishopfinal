                    @*<input type="checkbox" class="custom-control-input" checked="" id="price-all">
                        <label  for="price-all">All Price</label>*@

                    <a href="@Url.Action("showSPTheoNSX1",new {@MaLSP = item.MANSX})" class="dropdown-item"><img width="80" src="@Url.Content("~/Content/logoSP/"+item.Icon)" /> @item.TENNSX</a>
                    <hr />
                </div>
            }

        }

    </form>
</div>


